{
  "name": "als-simple-css",
  "version": "10.1.0",
  "description": "Simplify building CSS styles with JS.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "watch": "node ./scripts/build.js --watch",
    "readme": "node ./scripts/readme.js",
    "test": "node --test --experimental-test-coverage",
    "report": "node --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info"
  },
  "keywords": [
    "css",
    "javascript",
    "stylesheet",
    "dynamic",
    "runtime",
    "shortcut",
    "simple",
    "als-simple-css",
    "responsive",
    "interactive",
    "lightweight",
    "property-shortcut",
    "automation",
    "error-handling",
    "utility-methods"
  ],
  "author": "Alex Sorkin",
  "license": "MIT",
  "dependencies": {
    "als-color-tools": "^1.1.0",
    "als-css-parse": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexsorkin7/als-simple-css"
  }
}
